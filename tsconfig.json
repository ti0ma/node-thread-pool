{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "lib/*": ["./lib/*"]
    },
    "types": [
      "node"
    ],
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": false,
    "sourceMap": false,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules"
  ]
}
